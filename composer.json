{
    "name": "partivox/partivox",
    "description": "PartiVox Admin Dashboard",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "firebase/php-jwt": "6.4",
        "vlucas/phpdotenv": "^5.6",
        "mongodb/mongodb": "^2.0",
        "phpmailer/phpmailer": "^6.10",
        "abraham/twitteroauth": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "api/"
        },
        "files": [
            "api/config/db.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
